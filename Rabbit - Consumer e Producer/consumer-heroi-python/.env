# -------- RabbitMQ --------
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=myuser
RABBITMQ_PASSWORD=secret
RABBITMQ_VHOST=/
# Nome real da fila usada pelo produtor Spring (ajuste se necessário)
HEROI_QUEUE=overwatch_herois
RABBITMQ_PREFETCH=10
RABBITMQ_RECONNECT_DELAY=5

# -------- Database --------
DATABASE_URL=sqlite:///heroes_stats.db

# -------- Analytics --------
ANALYTICS_ENABLED=1
REPORT_INTERVAL=30  # (Ignorado no consumer mínimo)
SAVE_TO_FILE=0
OUTPUT_DIR=./reports

# -------- Webhook (opcional) --------
WEBHOOK_URL=
WEBHOOK_ENABLED=0

# -------- Discord Bot (opcional) --------
DISCORD_WEBHOOK_URL=
DISCORD_ENABLED=0
